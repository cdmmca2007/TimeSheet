/* classes for loadmask************************/
.load-mask{
	background: none repeat scroll 0 0 #DDDDDD;
    height: 100%;
    left: 0px;
    opacity: 0.7;
    position: absolute;
    top: 0px;
    width: 100%;
    z-index: 12000;
}
.load-mask-inner{
	background: none repeat scroll 0 0 #FDFDFD;
    border: 1px solid #BFBFBF;
    left: 48%;
    padding: 2px;
    position: absolute;
    top: 35%;
    z-index: 20001;
}
.load-msg {
    background-color: #EEEEEE;
    background-image: url("../images/ajax-loader.gif");
    background-position: 125px center;
    background-repeat: no-repeat;
    border: 1px solid #D0D0D0;
    color: #222222;
    height:20px;
    cursor: wait;
    font: 11px Helvetica Neue,Arial,sans-serif;
    padding: 20px 40px 5px 2px;
}
#stopEvent{
	z-index: 22000;
	float: right;
	margin: 3px 5px 3px 0;
	position: relative;
	height: 20px;
	width: 50px;
}
.load-msg{
	color:#000;
	font-weight: bold;
}
.mask-hidden{
	display: none;
}
