}
.usermenu-div {
	position: absolute;
	top: 39px;
	right: 10px;
	z-index: 1000;
}
.usermenu-div1 {
	position: absolute;
	top: 35px;
	right: 177px;
	z-index: 1000;
}
.usermenu-div2 {
	position: absolute;
	top: 35px;
	right: 10px;
	z-index: 1000;
}
.usermenu-div .info-inner ::before{
     content: "";
    position: absolute;
    bottom: 100%;
    left: 70%;
    margin-left: -20px;
    width: 0;
    height: 0;
    border-bottom: 16px solid rgba(0,0,0,0.2);
    border-right: 16px solid transparent;
